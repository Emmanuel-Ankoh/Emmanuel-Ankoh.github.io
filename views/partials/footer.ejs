<footer class="site-footer">
  <div class="container">
    <p>© <%= new Date().getFullYear() %> <%= (site && site.name) || 'Your Name' %>
      <% if (profile && profile.socials && profile.socials.github) { %> · <a href="<%= profile.socials.github %>" target="_blank" rel="noopener">GitHub</a><% } %>
      <% if (profile && profile.socials && profile.socials.linkedin) { %> · <a href="<%= profile.socials.linkedin %>" target="_blank" rel="noopener">LinkedIn</a><% } %>
      <% if (profile && profile.socials && profile.socials.twitter) { %> · <a href="<%= profile.socials.twitter %>" target="_blank" rel="noopener">Twitter</a><% } %>
      <% if (profile && profile.socials && profile.socials.instagram) { %> · <a href="<%= profile.socials.instagram %>" target="_blank" rel="noopener">Instagram</a><% } %>
      <% if (profile && profile.socials && profile.socials.youtube) { %> · <a href="<%= profile.socials.youtube %>" target="_blank" rel="noopener">YouTube</a><% } %>
      <% if (profile && profile.socials && profile.socials.stackoverflow) { %> · <a href="<%= profile.socials.stackoverflow %>" target="_blank" rel="noopener">Stack Overflow</a><% } %>
      <% if (profile && profile.socials && profile.socials.email) { %> · <a href="mailto:<%= profile.socials.email %>">Email</a><% } %>
    </p>
  </div>
</footer>
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
<script> AOS.init({ once: true }); </script>
<script src="/js/main.js"></script>
  </body>
</html>
